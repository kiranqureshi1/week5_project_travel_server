<% @cities.each do |city|%>
  <p>city: <%= city.name %><p/>
  <p>visit_status: <%= city.visit_status %></p>
  <p>number_of_visits: <%= city.number_of_visits %></p>
  <p>country: <%= city.find_country_name %></p>
  <p><a href="/travel/countries/cities/<%= city.id %>/edit">edit info</a></p>
  <form method="post" action="/travel/countries/cities">
    <input type="submit" value="Remove city"/>
  </form>
  <hr />
  <% end %>
